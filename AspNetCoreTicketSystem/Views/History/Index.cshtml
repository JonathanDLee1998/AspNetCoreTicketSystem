@model IEnumerable<AspNetCoreTicketSystem.Models.TicketSystem>

<h2>Ticket History</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Status</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Deleted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ticket in Model)
        {
                <tr>
                    <td>@ticket.Title</td>
                    <td>@ticket.Status</td>
                    <td>@ticket.CreatedAt.ToString("g")</td>
                    <td>@ticket.UpdatedAt.ToString("g")</td>
                    <td>@(ticket.IsDeleted ? "Yes" : "No")</td>
                </tr>
        }
    </tbody>
</table>
